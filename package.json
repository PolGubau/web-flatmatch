	"scripts": {
		"api:pull:edge": "npx supabase functions serve",
		"api:push:edge": "npx supabase functions deploy geocode --no-verify-jwt",
		"api:start": "supabase start",
		"api:stop": "supabase stop",
		"build": "react-router build",
		"coverage": "vitest run --coverage",
		"cz": " ",
		"db:copy-data": "npx supabase db dump --data-only > seed.sql && psql 'postgresql://postgres:postgres@127.0.0.1:15432/postgres' -f seed.sql",
		"db:pull": "npx supabase db pull",
		"db:push": "npx supabase db push",
		"dev": "react-router dev --mode dev",
		"dev-as-prod": "react-router dev --mode prod",
		"gen:types": "npx supabase gen types typescript --project-id \"eazucuralkbhkzrztanc\" --schema public > src/global/supabase/types.ts",
		"gen:types:local": "npx supabase gen types typescript --db-url \"postgresql://postgres:postgres@localhost:15432/postgres\" > src/global/supabase/types.ts",
		"go": "pnpm api:start && pnpm dev",
		"lint": "pnpm exec biome check ./src ./app",
		"lint-fix": "pnpm exec biome check --write --unsafe ./src ./app",
		"lint:ci": "pnpm exec biome ci ./src ./app",
		"release": "release-it",
		"start": "react-router-serve ./build/server/index.js",
		"test": "vitest",
		"typecheck": "react-router typegen && tsc"
	},
